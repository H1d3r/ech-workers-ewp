{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "listener": {
    "port": 8443,
    "address": "0.0.0.0",
    "modes": ["grpc"],
    "grpc_service": "ProxyService"
  },
  "protocol": {
    "type": "trojan",
    "password": "your-strong-password-here",
    "fallback": "127.0.0.1:80"
  },
  "tls": {
    "enabled": true,
    "cert_file": "/etc/ssl/certs/origin.pem",
    "key_file": "/etc/ssl/private/origin.key",
    "alpn": ["h2", "http/1.1"]
  },
  "advanced": {
    "enable_grpc_web": true
  }
}
