{
  "log": {
    "level": "info"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "ewp-tun",
      "inet4_address": "10.0.85.1/24",
      "mtu": 1380,
      "auto_route": true,
      "stack": "gvisor"
    }
  ],
  "outbounds": [
    {
      "type": "trojan",
      "tag": "trojan-grpc",
      "server": "trojan.example.com",
      "server_port": 443,
      "password": "your-password-here",
      "transport": {
        "type": "grpc",
        "service_name": "TrojanService",
        "idle_timeout": "15s",
        "health_check_timeout": "10s",
        "permit_without_stream": true,
        "initial_window_size": 4194304
      },
      "multiplex": {
        "enabled": true,
        "concurrency": 8,
        "padding": false
      },
      "tls": {
        "enabled": true,
        "server_name": "trojan.example.com",
        "alpn": ["h2"]
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "final": "trojan-grpc",
    "rules": [
      {
        "domain_suffix": [".cn", ".test"],
        "outbound": "direct"
      },
      {
        "ip_cidr": ["10.0.0.0/8", "192.168.0.0/16", "127.0.0.0/8"],
        "outbound": "direct"
      }
    ]
  }
}
