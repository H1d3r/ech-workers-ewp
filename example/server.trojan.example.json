{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "listener": {
    "port": 443,
    "address": "0.0.0.0",
    "modes": ["grpc", "ws"],
    "ws_path": "/",
    "grpc_service": "TrojanService"
  },
  "protocol": {
    "type": "trojan",
    "password": "your-password-here",
    "fallback": "127.0.0.1:80",
    "enable_flow": false
  },
  "tls": {
    "enabled": true,
    "cert_file": "/path/to/cert.pem",
    "key_file": "/path/to/key.pem",
    "alpn": ["h2", "http/1.1"]
  },
  "advanced": {
    "enable_grpc_web": true,
    "sse_headers": false
  }
}
