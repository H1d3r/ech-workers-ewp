{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "listener": {
    "port": 443,
    "address": "0.0.0.0",
    "modes": ["h3", "grpc", "ws"],
    "ws_path": "/ws",
    "grpc_service": "api"
  },
  "protocol": {
    "type": "ewp",
    "uuid": "d342d11e-d424-4583-b36e-524ab1f0afa4",
    "enable_flow": true
  },
  "tls": {
    "enabled": true,
    "cert_file": "/path/to/cert.pem",
    "key_file": "/path/to/key.pem",
    "alpn": ["h3", "h2", "http/1.1"],
    "min_version": "1.2",
    "max_version": "1.3"
  },
  "advanced": {
    "enable_grpc_web": true,
    "padding_min": 100,
    "padding_max": 1000,
    "sse_headers": true,
    "h3_max_bidi_streams": 100,
    "h3_max_uni_streams": 100
  }
}
